<?xml version="1.1" encoding="UTF-8"?>
<ehcache name="shirocache" updateCheck="false">
    <diskStore path="java.io.tmpdir"/>
     <defaultCache 
        maxElementsInMemory="10000"
        eternal="true"
        overflowToDisk="true"/>

    <cache name="fileCreateQueue"
           maxElementsInMemory="20000"
           eternal="true"
           diskPersistent="true"
           overflowToDisk="true">
    </cache>
</ehcache>